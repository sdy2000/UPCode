@model Core.DTOs.RegisterViewModel
@{
    ViewData["Title"] = "End of registration";
}

<div class="container">
    <nav aria-label="breadcrumb">
        <ul class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">UPCode</a></li>
            <li class="breadcrumb-item active" aria-current="page">End of registration</li>
        </ul>
    </nav>
</div>



<main class="client-page">
    <div class="container-content">
        <header><h2> End of registration </h2></header>
        <hr />
        <div class="alert alert-success">
            <h3>Hi @Model.UserName .</h3>
            <p>
                Your registration is successful, an email with an activation link has been sent to @Model.Email.
            </p>
            <p>
                To continue the registration process, enter your email and activate your account!
            </p>
        </div>
    </div>
</main>